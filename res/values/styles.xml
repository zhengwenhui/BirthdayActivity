<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--  -->
	<style
		name="translucent"
		parent="@android:style/Theme.Dialog" />

	<style
		name="CustomWindowTitleBackground">
		<item
			name="android:background">@drawable/title</item>
	</style>

	<style
		name="setBirthdaystyle"
		parent="android:Theme">
		<item
			name="android:windowTitleSize">50dp</item>
		<item
			name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
	</style>
	<style
		name="noTitleBar"
		parent="android:Theme"
		>
		<item
			name="android:windowNoTitle">true</item>
	</style>
</resources>
